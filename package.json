{
  "name": "mable-code-test",
  "version": "1.0.0",
  "description": "Simple banking system",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "node ./dist/app.js",
    "dev": "nodemon src/app.ts",
    "build": "tsc -p .",
    "prettier": "prettier --write src/**/*.ts",
    "lint": "npm run prettier && eslint src/**/*.ts",
    "test": "npx jest",
    "docker:prod:setup": "docker compose up",
    "docker:setup": "docker pull mongo && docker run --name mongo-test -d -p 27018:27017 mongo:7",
    "docker:delete": " docker rm -f mongo-test",
    "docker:stop": "docker stop mongo-test",
    "docker:start": "docker start mongo-test"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.9.0",
    "cookie-parser": "^1.4.6",
    "csv-parse": "^3.2.0",
    "dotenv": "^16.5.0",
    "express": "^4.18.2",
    "http-errors": "^2.0.0",
    "mongodb": "^6.17.0",
    "mongoose": "^8.15.1",
    "morgan": "^1.10.0",
    "winston": "^3.9.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.28.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/debug": "^4.1.8",
    "@types/express": "^4.17.17",
    "@types/http-errors": "^2.0.1",
    "@types/jest": "^29.5.14",
    "@types/morgan": "^1.9.4",
    "@typescript-eslint/eslint-plugin": "^5.59.7",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "globals": "^16.2.0",
    "jest": "^29.7.0",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.8",
    "ts-jest": "^29.3.4",
    "ts-node": "^10.9.1",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.33.1"
  }
}
